<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:windowBackground">@color/background_floating_material_light</item>
        <item name="android:colorBackground">@color/background_floating_material_light</item>
    </style>
    <style name="ButtonStyle">
        <item name="android:background">@drawable/onclickcolor</item>
        <item name="android:textColor">#ECECEA</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="colorButtonNormal">#D9853B</item>
    </style>

    <style name="RepartoManager" parent="AppTheme" >
        <item name="android:colorBackground">@color/background_material_light</item>
    </style>

</resources>